        var $form = $("#customer-form");
        if ($form.length) {
            $form.on("submit", function (e) {
                e.preventDefault();
                $form.find(".msg").remove();
                $.post("/api/v1/dashboard/crm/addCustomer", $form.serialize(), function (res) {
                    if (res.errors) {
                        console.log(res.errors);
                        $.each(res.errors, function (i, err) {
                            $form
                                .find("[name=" + err.param + "]")
                                .after('<div class="msg error">' + err.msg + "</div>");
                        });
                    } else {
                        console.log("Success");

                        let d = document.getElementById('con-close-modal')
                        d.style.display = "none"
                        $("#con-close-modal").modal('hide');
                     



                       
                        //id van de customers div = de div waar het letterlijk 'in' komt netals cart
                        $("#customers").append(res.data);





                        toastr.success("Klant is aangemaakt.")

                        toastr.options = {
                        "closeButton": false,
                        "debug": false,
                        "newestOnTop": true,
                        "progressBar": true,
                        "positionClass": "toast-top-right",
                        "preventDuplicates": false,
                        "onclick": null,
                        "showDuration": "300",
                        "hideDuration": "1000",
                        "timeOut": "5000",
                        "extendedTimeOut": "1000",
                        "showEasing": "swing",
                        "hideEasing": "linear",
                        "showMethod": "fadeIn",
                        "hideMethod": "fadeOut"
                        }
                        //window.location.href = res.url;
                    }
                });
            });
        }
